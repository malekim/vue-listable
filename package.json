{
  "name": "vue-listable",
  "version": "0.0.6",
  "description": "An advanced data table plugin for vue.js and nuxt.js",
  "main": "build/vue-listable.umd.js",
  "module": "build/vue-listable.esm.js",
  "unpkg": "build/vue-listable.min.js",
  "scripts": {
    "lint": "eslint --ext .ts --ignore-path .gitignore .",
    "build": "NODE_ENV=production && npm run build-ts && npm run build-scss",
    "build-ts": "rollup -c rollup.config.ts",
    "build-scss": "sass --style=compressed src/scss/listable.scss:build/vue-listable.min.css",
    "test": "jest",
    "docs:dev": "vuepress dev",
    "docs:build": "vuepress build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/malekim/vue-listable.git"
  },
  "keywords": [
    "vue",
    "vuejs",
    "plugin",
    "datatable",
    "table",
    "list",
    "grid"
  ],
  "author": "Lukasz Malek <dev@malek.im>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/malekim/vue-listable/issues"
  },
  "homepage": "https://malekim.github.io/vue-listable",
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@babel/preset-env": "^7.7.6",
    "@rollup/plugin-typescript": "^5.0.0",
    "@types/jest": "^26.0.0",
    "@typescript-eslint/eslint-plugin": "^3.3.0",
    "@typescript-eslint/parser": "^3.3.0",
    "@vue/test-utils": "^1.0.0-beta.30",
    "acorn": "^7.1.0",
    "acorn-dynamic-import": "^4.0.0",
    "babel-jest": "^24.9.0",
    "eslint": "^7.3.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.0.1",
    "jest-serializer-vue": "^2.0.2",
    "prettier": "^2.0.5",
    "rollup": "^1.27.9",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-terser": "^5.3.0",
    "sass": "^1.26.9",
    "serialize-javascript": ">=2.1.1",
    "ts-jest": "^26.1.0",
    "typescript": "^3.9.5",
    "vue": "^2.6.10",
    "vue-jest": "^3.0.5",
    "vue-template-compiler": "^2.6.10",
    "vue-test-utils": "^1.0.0-beta.11"
  }
}

{
  "name": "vue-listable",
  "version": "0.0.7",
  "description": "An advanced data table plugin for vue.js and nuxt.js",
  "main": "build/vue-listable.umd.js",
  "module": "build/vue-listable.esm.js",
  "unpkg": "build/vue-listable.min.js",
  "scripts": {
    "build": "NODE_ENV=production rollup -c && node-sass --include-path scss src/scss/listable.scss build/vue-listable.min.css --output-style=compressed",
    "watch": "rollup -c -w",
    "test": "jest",
    "docs:dev": "vuepress dev",
    "docs:build": "vuepress build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/malekim/vue-listable.git"
  },
  "keywords": [
    "vue",
    "vuejs",
    "plugin",
    "datatable",
    "table",
    "list",
    "grid"
  ],
  "author": "Lukasz Malek <dev@malek.im>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/malekim/vue-listable/issues"
  },
  "homepage": "https://malekim.github.io/vue-listable",
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@babel/preset-env": "^7.7.6",
    "@vue/test-utils": "^1.0.0-beta.30",
    "acorn": "^7.1.0",
    "acorn-dynamic-import": "^4.0.0",
    "babel-jest": "^24.9.0",
    "eslint": "^6.7.2",
    "jest": "^24.9.0",
    "jest-serializer-vue": "^2.0.2",
    "node-sass": "^4.13.0",
    "rollup": "^1.27.9",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-buble": "^0.19.8",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-terser": "^5.1.2",
    "serialize-javascript": ">=2.1.1",
    "vue": "^2.6.10",
    "vue-jest": "^3.0.5",
    "vue-template-compiler": "^2.6.10",
    "vue-test-utils": "^1.0.0-beta.11"
  }
}
